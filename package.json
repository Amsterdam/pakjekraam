{
  "name": "@fixxx/pakjekraam",
  "version": "0.0.0",
  "description": "",
  "repository": "https://github.com/Amsterdam/fixxx-pakjekraam.git",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "gulp build",
    "build:typescript": "tsc --project tsconfig.json",
    "deploy": "time node src/deploy.js",
    "clean": "rm -rf dist/* node_modules/.cache/",
    "prestart": "npm run deploy",
    "start": "ts-node src/index.js",
    "predev": "npm run build && npm run deploy",
    "dev": "ts-node-dev src/index.js",
    "allocation": "ts-node src/allocation.js",
    "tdd": "jest --watch --verbose --rootDir ./src/",
    "test": "jest --verbose --silent --rootDir ./src/",
    "lint": "concurrently 'npm:lint:*'",
    "lint:editorconfig": "git ls-files '*.cfg' '*.cnf' '*.conf' '*.crt' '*.css' '*.csv' '*.dockerignore' '*.editorconfig' '*.env' '*.eslintignore' '*.eslintrc' '*.feature' '*.gitattributes' '*.gitignore' '*.groovy' '*.htm' '*.html' '*.ini' '*.json' '*.key' '*.less' '*.markdown' '*.md' '*.params' '*.pem' '*.php' '*.pub' '*.py' '*.rb' '*.rdf' '*.rq' '*.rsa' '*.scss' '*.service' '*.sh' '*.spec' '*.sql' '*.srl' '*.stylelintrc' '*.svg' '*.twig' '*.txt' '*.vcl' '*.wsdl' '*.xhtml' '*.xliff' '*.xml' '*.xqy' '*.xsd' '*.xsl' '*.xslt' '*.yml' '*AUTHORS' '*CONTRIBUTORS' '*Dockerfile' '*LICENSE' '*Makefile' '*README' '*Jenkinsfile' | xargs lintspaces --matchdotfiles --newline --editorconfig .editorconfig --ignores js-comments",
    "lint:eslint": "eslint './src/**/*.js' './src/**/*.jsx'",
    "lint:tslint": "tslint --type-check --project tsconfig.json --config tslint.json './src/**/*.ts' './src/**/*.tsx'",
    "lint-fix": "npm run lint-fix:eslint; npm run lint-fix:prettier",
    "lint-fix:eslint": "npm run lint:eslint -- --fix",
    "lint-fix:prettier": "prettier --write '**/*.js' '**/*.css' '**/*.scss' '**/*.json' '**/*.jsx' '*.yml'",
    "lint-fix:tslint": "npm run lint:tslint -- --fix",
    "lint-non-npm": "git ls-files '*.yml' | tr '\n' '\\0' | xargs -0 yamllint",
    "prewatch": "npm run clean && npm run build",
    "import": "node src/import-makkelijke-markt.js",
    "mailing-list": "ts-node src/mailing-list.js",
    "watch": "gulp watch"
  },
  "author": "",
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/bluebird": "3.5.27",
    "@types/jest": "23.3.10",
    "@types/node": "11.11.5",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/validator": "10.11.0",
    "babel-eslint": "10.0.1",
    "concurrently": "4.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-react": "7.13.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.2.0",
    "gulp-sass": "4.0.2",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.7",
    "lintspaces-cli": "0.7.1",
    "node-dev": "4.0.0",
    "node-sass": "4.12.0",
    "npm-check": "5.9.0",
    "prettier": "1.17.1",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.12.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "axios": "0.19.0",
    "axios-cache-adapter": "2.3.2",
    "axios-logger": "1.0.0",
    "body-parser": "1.19.0",
    "connect-ensure-login": "0.1.1",
    "connect-pg-simple": "5.0.0",
    "cookie-parser": "1.4.4",
    "express": "4.17.1",
    "express-react-views": "0.11.0",
    "express-session": "1.16.1",
    "morgan": "1.9.1",
    "nodemailer": "6.2.1",
    "nodemailer-sendgrid-transport": "0.2.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.11.0",
    "pg-hstore": "2.3.3",
    "pg-pool": "2.0.6",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-native": "0.59.9",
    "sequelize": "5.8.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{css,js,json,jsx,scss,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
